[
  {
    "kernelSource": {
      "name": "RedmiPad",
      "repo": "https://github.com/Arpitshubhamm/kernel_xiaomi-yunluo.git",
      "branch": "udc",
      "device": "yunluo",
      "defconfig": "gki_defconfig"
    },
    "enableCcache": true,
  "toolchains": [],
  "params": {
    "ARCH": "arm64",
    "CC": "aarch64-linux-gnu-clang",
    "externalCommands": {
      "CROSS_COMPILE": "aarch64-linux-gnu-",
      "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
      "LD": "aarch64-linux-gnu-ld",
      "AR": "arm-linux-gnueabi-ar",
      "NM": "arm-linux-gnueabi-nm",
      "OBJCOPY": "arm-linux-gnueabi-objcopy",
      "OBJDUMP": "arm-linux-gnueabi-objdump",
      "READELF": "arm-linux-gnueabi-readelf",
      "OBJSIZE": "arm-linux-gnueabi-size",
      "STRIP": "arm-linux-gnueabi-strip",
      "LDGOLD": "aarch64-linux-gnu-ld.gold",
      "LLVM_AR": "arm-linux-gnueabi-ar",
      "LLVM_DIS": "arm-linux-gnueabi-dis"
    }
   },

    "AnyKernel3": {
      "use": true,
      "release": true
    },
    "enableKernelSU": true,
    "enableLXC": false
  }
]
